{
  "name": "@mrjasonroy/cache-components-cache-handler",
  "version": "16.0.2",
  "description": "Cache handler for Next.js 16+ with support for 'use cache' directive and Cache Components",
  "author": "Jason Roy",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "keywords": [
    "nextjs",
    "next",
    "cache",
    "cache-handler",
    "use-cache",
    "isr",
    "revalidation",
    "redis",
    "elasticache",
    "valkey"
  ],
  "peerDependencies": {
    "next": "~16.0.0"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrjasonroy/cache-components-cache-handler.git",
    "directory": "packages/cache-handler"
  },
  "homepage": "https://github.com/mrjasonroy/cache-components-cache-handler#readme",
  "bugs": {
    "url": "https://github.com/mrjasonroy/cache-components-cache-handler/issues"
  }
}
